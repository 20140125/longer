{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-zip": "*",
        "baidu/aip-sdk": "^2.2",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.4",
        "friendsofphp/php-cs-fixer": "^3.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "php-curl-class/php-curl-class": "^9.1",
        "php-webdriver/webdriver": "^1.11",
        "predis/predis": "^1.1",
        "w7corp/easywechat": "^5.16",
        "workerman/gateway-worker" : "v3.0.23",
        "workerman/globaldata": "^1.0",
        "workerman/mysql": "^1.0",
        "workerman/phpsocket.io": "^1.1",
        "workerman/workerman": "^4.0.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "vendor/baidu/aip-sdk"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
